# Development Environment Configuration
# Branch: dev
# URL: dev.forgespace.co

# Node Environment
NODE_ENV=development

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://dev.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=dev_anon_key_placeholder
DATABASE_URL=postgresql://dev_user:dev_pass@localhost:5432/dev_db
SUPABASE_SERVICE_ROLE_KEY=dev_service_key_placeholder

# API Configuration
API_BASE_URL=https://dev.forgespace.co/api
LOG_LEVEL=debug

# Feature Flags
FEATURE_FLAGS=NEW_AI_MODELS,ADVANCED_COMPONENTS,BETA_FEATURES,DEBUG_TOOLS,EXPERIMENTAL_UI

# Development Settings
ENABLE_DEBUG_TOOLS=true
ENABLE_SOURCE_MAPS=true
ENABLE_HOT_RELOAD=true
ENABLE_MOCK_DATA=true

# Rate Limiting (relaxed for dev)
API_RATE_LIMIT=1000

# Monitoring (enabled but less strict)
MONITORING_ENABLED=true
ANALYTICS_ENABLED=false

# Security (relaxed for dev)
CORS_ORIGIN=*
ENABLE_DEBUG_ENDPOINTS=true
SKIP_AUTHENTICATION=false
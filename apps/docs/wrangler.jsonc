{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": ".open-next/worker.js",
  "name": "siza-docs",
  "compatibility_date": "2024-12-30",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS",
  },
  "env": {
    "dev": {
      "name": "siza-docs-dev",
      "routes": [
        {
          "pattern": "dev-docs.forgespace.co",
          "custom_domain": true,
        },
      ],
    },
  },
}
